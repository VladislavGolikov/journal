doctype html
html(lang='ru')
  head
    title Журнал для записи показаний приёмников в пультовой
    meta(charset='UTF-8')
    link(rel='icon' type='image/png' href='favicon.png')
    script(defer='defer' type='module' src='index.js')
  body
    .logo
      img.logotype(src='./icons/logo.svg' alt='gazprom')
    button.changePage
      .caption листать
    button.changeMonthForward
      .caption
        | месяц
        br
        | вперед
    button.changeMonthBackward
      .caption месяц назад
    button.printPage
      .caption печать
    .allthereis

